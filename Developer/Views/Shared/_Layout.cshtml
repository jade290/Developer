<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/Images/favicon-fire.ico" rel="shortcut icon" type="image/x-icon"/>
    <!--CSS styles-->
    <link href="~/Client/vendor/font-awesome-4.5.0/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Client/vendor/Bootstrap-3.3.6/css/bootstrap.min.css" rel="stylesheet"/>
    <link href="~/Client/vendor/SiteOwner/css/perfect-scrollbar-0.4.5.min.css" rel="stylesheet" />
    <link href="~/Client/vendor/SiteOwner/css/magnific-popup.css" rel="stylesheet" />
    <link href="~/Client/vendor/SiteOwner/css/style.css" rel="stylesheet" />
    <link href="~/Client/vendor/SiteOwner/css/custom-style.css" rel="stylesheet" />
    <link id="theme-style" href="~/Client/vendor/SiteOwner/css/default.css" rel="stylesheet"/>
    <link href="~/Client/vendor/tag-it-master/css/jquery.tagit.css" rel="stylesheet"/>
    <link href="~/Client/vendor/tag-it-master/css/tagit.ui-zendesk.css" rel="stylesheet" />
    
    <!--Javascript files-->
    <script src="~/Client/vendor/jquery-2.1.4.js" type="text/javascript"></script>
    <script src="~/Client/vendor/jquery-ui-1.11.4.js" type="text/javascript"></script>
    <script src="~/Client/vendor/SiteOwner/js/TweenMax.min.js" type="text/javascript"></script>
    <script src="~/Client/vendor/SiteOwner/js/jquery.touchSwipe.min.js"></script>
    <script src="~/Client/vendor/SiteOwner/js/jquery.mixitup.min.js" type="text/javascript"></script>
    <script src="~/Client/vendor/SiteOwner/js/jquery.carouFredSel-6.2.1-packed.js" type="text/javascript"></script>
    <script src="~/Client/vendor/SiteOwner/js/jquery.dropdownit.js" type="text/javascript"></script>
    <script src="~/Client/vendor/SiteOwner/js/jquery.stellar.min.js" type="text/javascript"></script>
    <script src="~/Client/vendor/SiteOwner/js/ScrollToPlugin.min.js" type="text/javascript"></script>
    <script src="~/Client/vendor/Bootstrap-3.3.6/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Client/vendor/SiteOwner/js/jquery.mixitup.min.js" type="text/javascript"></script>
    <script src="~/Client/vendor/SiteOwner/js/masonry.min.js" type="text/javascript"></script>
    <script src="~/Client/vendor/SiteOwner/js/perfect-scrollbar-0.4.5.with-mousewheel.min.js" type="text/javascript"></script>
    <script src="~/Client/vendor/SiteOwner/js/magnific-popup.js" type="text/javascript"></script>
    <script src="~/Client/vendor/SiteOwner/js/custom.js" type="text/javascript"></script>
    <script src="~/Client/vendor/tag-it-master/js/tag-it.min.js" type="text/javascript"></script>

    <!-- SEO -->
    <script type="application/ld+json">
    {
      "@@context": "http://schema.org",
      "@@type": "WebSite",
      "url": "http://www.lori.avoratech.com/",
      "name": "Lori M. Collier - Ph 205 276 5306",
      "author": {
            "@@type": "Person",
            "honorificPrefix": "Mrs.",
            "name": "Lori Collier",
            "honorificSuffix": "MBA, MSCS, Security+, Network+, A+"
       }
    }
    </script>
</head>
<body>
    <div id="wrapper">
        <a class="mobilemenu" href="#sidebar"><i class="icon-reorder"></i></a>
        <div id="sidebar">
            <div id="main-nav">
                <div id="nav-container">
                    <div class="clearfix" id="profile">
                        <div class="portrate hidden-xs"></div>
                        <div class="title">
                            <h2>Lori Collier</h2>
                            <h3>CEO, Avora Technology Consultants</h3>
                        </div>
                    </div>
                    <ul id="navigation">
                        <li>
                            <a href="#biography">
                                <div class="icon"><i class="fa fa-user"></i></div>
                                <div class="text">About Me</div>
                            </a>
                        </li>
                        <li>
                            <a href="#research">
                                <div class="icon"><i class="fa fa-book"></i></div>
                                <div class="text">Research</div>
                            </a>
                        </li>
                        <li>
                            <a href="#teaching">
                                <div class="icon"><i class="fa fa-hourglass"></i></div>
                                <div class="text">Teaching</div>
                            </a>
                        </li>
                        <li>
                            <a href="#contact">
                                <div class="icon"><i class="fa fa-calendar"></i></div>
                                <div class="text">Contact &amp; Meet Me</div>
                            </a>
                        </li>
                        <li class="external">
                            <a href="https://drive.google.com/file/d/0BzYmv15h09yvNGF1d3owXzZuQmM/view?usp=sharing" target="_blank">
                                <div class="icon"><i class="fa fa-cloud-download"></i></div>
                                <div class="text">Download CV</div>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="social-icons">
                <ul>
                    <li>
                        <a href="#">
                        </a>
                    </li>
                    <li>
                        <a href="google.com/+LoriCollier1" target="_blank">
                            <i class="fa fa-google-plus"></i>
                        </a>
                    </li>
                    <li>
                        <a href="https://www.linkedin.com/in/lorimcollier" target="_blank">
                            <i class="fa fa-linkedin"></i>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    @RenderBody()
    @RenderSection("scripts", required: false)
</body>
</html>
